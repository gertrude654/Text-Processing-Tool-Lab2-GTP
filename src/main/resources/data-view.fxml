<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.textprocessingtool.data.DataManagerController" spacing="10" padding="10, 10, 10, 10">
    <TextField fx:id="keyField" promptText="Enter Key"/>
    <TextField fx:id="valueField" promptText="Enter Value"/>
    <Button text="Add to List" onAction="#handleAddToList"/>
    <Button text="Add to Set" onAction="#handleAddToSet"/>
    <Button text="Add to Map" onAction="#handleAddToMap"/>
    <Button text="Show List" onAction="#handleShowList"/>
    <Button text="Show Set" onAction="#handleShowSet"/>
    <Button text="Show Map" onAction="#handleShowMap"/>
    <TextArea fx:id="outputArea" prefHeight="200" wrapText="true"/>
</VBox>
